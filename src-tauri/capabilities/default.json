{
  "$schema": "../gen/schemas/desktop-schema.json",
  "name": "default",
  "identifier": "default-features",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:default",
    "dialog:default",
    {
      "identifier": "fs:allow-write-file",
      "allow": [
        {
          "path": "**"
        }
      ]
    },
    {
      "identifier": "fs:allow-write-text-file",
      "allow": [
        {
          "path": "**"
        }
      ]
    }
  ]
}